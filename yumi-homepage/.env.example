# Datadog RUM Configuration
# Copy this file to .env and fill in your actual values

# Datadog Application ID (required)
VITE_DATADOG_APPLICATION_ID=your_datadog_app_id_here

# Datadog Client Token (required)
VITE_DATADOG_CLIENT_TOKEN=your_datadog_client_token_here

# Datadog Site (default: datadoghq.com)
VITE_DATADOG_SITE=datadoghq.com

# Service name for tracking
VITE_DATADOG_SERVICE=yumi-homepage

# Environment (development, staging, production)
VITE_DATADOG_ENV=development

# Application version
VITE_DATADOG_VERSION=1.0.0

# Enable session replay (true/false)
VITE_DATADOG_SESSION_REPLAY=true

# Sample rate for RUM (0-100)
VITE_DATADOG_SAMPLE_RATE=100
